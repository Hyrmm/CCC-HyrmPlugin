{
  "name": "hyrm-plugin",
  "scene-script": "walker/index.js",
  "version": "0.0.3",
  "description": "解决日常繁琐交互的插件,工具合集",
  "author": "hyrm",
  "main": "main.js",
  "main-menu": {
    "i18n:hyrm-plugin.menu-title/i18n:hyrm-plugin.sort-nodes": {
      "message": "hyrm-plugin:sort-nodes/sort",
      "accelerator": "F4"
    },
    "i18n:hyrm-plugin.menu-title/i18n:hyrm-plugin.watch-file": {
      "message": "hyrm-plugin:watch-file/onwatch",
      "accelerator": "F5"
    },
    "i18n:hyrm-plugin.menu-title/i18n:hyrm-plugin.open-spine-preview": {
      "message": "hyrm-plugin:spine-preview/open"
    },
    "i18n:hyrm-plugin.menu-title/i18n:hyrm-plugin.global-setting": {
      "message": "hyrm-plugin:panel/open-setting"
    }
  },
  "panel": {
    "main": "panel/index.js",
    "type": "fixed-size",
    "title": "i18n:hyrm-plugin.global-setting-title",
    "width": 450,
    "height": 500
  },
  "dependencies": {
    "pubsub.js": "^1.5.2",
    "watch": "^1.0.2"
  }
}